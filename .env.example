PORT=3001

# Base URL of the HopShare backend (defaults to http://localhost:3000)
REACT_APP_API_BASE=http://localhost:3000

# Toggle basic auth usage (true/false). If false, will skip Authorization header.
REACT_APP_BASIC_AUTH_ENABLED=true

# Optional default seed credentials for quick local testing
REACT_APP_DEMO_EMAIL=alice@example.com
REACT_APP_DEMO_PASSWORD=password123

# Base URL for Cypress end-to-end tests
CYPRESS_BASE_URL=http://localhost:3001

